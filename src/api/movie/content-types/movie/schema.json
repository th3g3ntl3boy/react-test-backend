{
  "kind": "collectionType",
  "collectionName": "movies",
  "info": {
    "singularName": "movie",
    "pluralName": "movies",
    "displayName": "MOVIE",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "text",
      "required": true
    },
    "release_date": {
      "type": "date",
      "required": true
    },
    "category": {
      "type": "enumeration",
      "enum": [
        "Action",
        "Comedy",
        "School",
        "Romance",
        "Horor",
        "Sci-Fi",
        "Conscpiracy",
        "Adventure",
        "Drama",
        "Historical",
        "Musical"
      ],
      "required": true
    },
    "Region": {
      "type": "enumeration",
      "enum": [
        "ID",
        "NOTID"
      ],
      "required": true
    },
    "ytlink": {
      "type": "string",
      "required": true
    },
    "view": {
      "type": "integer",
      "default": 0,
      "min": 0
    },
    "linkgambar": {
      "type": "text",
      "required": true
    },
    "comments": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::comment.comment",
      "mappedBy": "movie"
    },
    "histories": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::history.history",
      "mappedBy": "movie"
    },
    "ratings": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::rating.rating",
      "mappedBy": "movie"
    },
    "bookmarks": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::bookmark.bookmark",
      "mappedBy": "movie"
    },
    "categories": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::category.category",
      "inversedBy": "movies"
    }
  }
}
